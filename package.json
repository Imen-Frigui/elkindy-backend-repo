{
  "name": "elkindy_backend",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app.js",
    "test:jest": "cross-env NODE_ENV=dev jest --config=tests/jest.config.js --detectOpenHandles --forceExit --coverage",
    "test:mocha": "cross-env NODE_ENV=test nyc  --reporter=lcov --reporter=text-summary mocha tests/course.test.js --timeout 10000 --reporter spec --exit",
    "test": "cross-env NODE_ENV=test npm run test:mocha && npm run test:jest",
    "coverage": "nyc merge coverage/ coverage-final.json && nyc report --reporter=lcov --reporter=text-summary"
  },
  "engines": {
    "node": "12.8.0"
  },
  "author": "NetRunners",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.17.1",
    "jest": "^29.7.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "mongoose": "^8.1.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-uuid": "^1.4.8",
    "nodemon": "^3.0.3",
    "socket.io": "^4.7.4",
    "supertest": "^6.3.4",
    "swagger-jsdoc": "^3.4.0",
    "swagger-ui-express": "^4.1.1",
    "sync-request": "^6.1.0",
    "validator": "13.11.0",
    "winston": "^3.11.0",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "chai": "4.4.1",
    "chai-http": "^4.4.0",
    "cross-env": "^7.0.3",
    "mocha": "^10.7.3",
    "nyc": "^17.1.0"
  },
  "optionalDependencies": {
    "fsevents": "*"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "extension": [
      ".js"
    ],
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "tests/**/*.js"
    ],
    "all": true
  }
}
